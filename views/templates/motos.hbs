<div class="container mt-5">
<div class="card">
  <table class="table table-sm">
    <thead class="table-dark">
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Marca</th>
        <th scope="col">Modelo</th>     
        <th scope="col">Cor</th>
        <th scope="col">Ano</th>
        <th scope="col">Placa</th>
        <th scope="col">Pre√ßo(R$)</th>
      </tr>
     
    </thead>
    <tbody>
    {{#each motos}}
      <tr>
      <th scope="row">{{this.id_moto}}</th>
      <td>{{this.marca}}</td>
      <td>{{this.modelo}}</td>  
      <td>{{this.cor}}</td>
      <td>{{this.ano}}</td>
      <td>{{this.placa}}</td>
      <td>{{this.preco}}</td>
    </tr> 
    {{/each}}
  </tbody>
</table>
</div>
</div>